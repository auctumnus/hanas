{
  "name": "hanas-frontend",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "prettier --write .",
    "lint:check": "prettier --check .",
    "commit": "git-cz"
  },
  "dependencies": {
    "@vueuse/core": "4.11.2",
    "reset.css": "2.0.2",
    "vue": "3.1.4",
    "vuex": "4.0.2",
    "vuex-persist": "3.1.3"
  },
  "devDependencies": {
    "@iconify/json": "1.1.376",
    "@vitejs/plugin-vue": "1.2.5",
    "@vue/compiler-sfc": "3.1.4",
    "@vue/server-renderer": "3.1.4",
    "@vueuse/head": "0.6.0",
    "commitizen": "4.2.4",
    "git-cz": "4.7.6",
    "prettier": "2.3.0",
    "simple-git-hooks": "2.5.1",
    "typescript": "4.3.5",
    "vite": "2.4.2",
    "vite-plugin-components": "0.12.2",
    "vite-plugin-favicon": "1.0.8",
    "vite-plugin-icons": "0.6.5",
    "vite-plugin-pages": "0.15.0",
    "vite-plugin-windicss": "1.2.4",
    "vue-router": "4.0.10",
    "vue-tsc": "0.2.1"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint"
  }
}
